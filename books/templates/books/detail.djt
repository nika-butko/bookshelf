{% extends 'base.djt' %}

{% block content %}
<div class="container">  
    
    <h1>{{ book.title }}</h1>
 
    <p>{{ book.authors.all|join:", " }}</p>
       
    <p>{{ book.series.all|join:", " }}</p>

    <p>{{ book.teaser }}</p>
    <p>{{ book.description }}</p>
</div><!-- /.container -->
{% endblock %}